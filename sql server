create database tickeTime;
go
use tickeTime;
go

if OBJECT_ID('INTERACCIONES') is not null
drop table INTERACCIONES;
go
if OBJECT_ID('QRASISTENCIA') is not null
drop table QRASISTENCIA;
go
if OBJECT_ID('PROVEEDORDESERVICIOS') is not null
drop table PROVEEDORDESERVICIOS;
go
if OBJECT_ID('SEGUIMIENTO') is not null
drop table SEGUIMIENTO;
go
if OBJECT_ID('SERVICIOS') is not null
drop table SERVICIOS;
go
if OBJECT_ID('COMPRASTICKETS') is not null
drop table COMPRASTICKETS;
go
if OBJECT_ID('EVENTOS') is not null
drop table EVENTOS;
go
if OBJECT_ID('USUARIOS') is not null
drop table USUARIOS;
go
if OBJECT_ID('CATEGORIAS') is not null
drop table CATEGORIAS;
go
create table USUARIOS(
idUsuario int primary key not null,
nombres nvarchar(30) not null,
apellidos nvarchar(30) not null,
usuario varchar(30) not null,
pass varchar(10) not null,
edad int not null,
correo varchar(30) not null,
rol varchar(20) not null,
politicaData char(1) not null,
noSeguidos int,
razonSocial nvarchar (30),
noSeguidores int
);
create table SERVICIOS(
idServicios varchar (3) primary key not null,
descripcion nvarchar(100) not null,
);
create table PROVEEDORDESERVICIOS(
idOfertaServicio varchar (12) primary key not null,
idProveedor int not null,
servicio varchar(3) not null,
descripcion varchar(200) not null,
imagen image,
foreign key (idProveedor) references USUARIOS (idUsuario),
foreign key (servicio) references SERVICIOS (idServicios)
);

create table SEGUIMIENTO(
idSeguimiento int identity primary key,
seguido int not null,
seguidor int not null,
foreign key (seguido) references USUARIOS (idUsuario),
foreign key (seguidor) references USUARIOS (idUsuario),
);

create table CATEGORIAS(
idCategoria varchar (10) primary key not null,
descripcion varchar (50) not null
);

create table EVENTOS(
idEventos varchar (10) primary key not null,
idOrganizador int not null,
nomEvent nvarchar(30) not null,
descripcion nvarchar(800) not null,
lugar varchar(30) not null,
modalidad varchar(30) not null,
MedioTransm varchar(30),
enlConexion varchar(50),
Presentador varchar(50) not null,
direccion varchar(50),
fech_Ini date not null,
hora_Ini varchar(5) not null,
fech_Fin date not null,
hora_Fin varchar(5) not null,
intLucro varchar(30) not null,
costo float not null,
aforo int not null, 
totalVendidos int not null,
PlazasDisponibles int not null,
ubicacion varchar (2),
categoria varchar (10) not null,
estado varchar (10) not null,
img  image,
numLikes int not null
foreign key (idOrganizador) references USUARIOS (idusuario),
foreign key (categoria) references CATEGORIAS (idCategoria),
);

create table COMPRASTICKETS(
idCompras varchar (10) primary key not null,
idEvento varchar(10) not null,
comprador int not null,
cant int,
total float not null,
foreign key (idEvento) references EVENTOS (idEventos),
foreign key (comprador) references USUARIOS (idUsuario)
);
create table QRASISTENCIA(
idQr varchar (10) primary key not null,
transaccion varchar(10) not null,
qr varchar (10) not null,
asistencia char(1) not null,
foreign key (transaccion) references COMPRASTICKETS (idCompras)
);
create table INTERACCIONES(
idInteraccion varchar (10) primary key not null,
qrAsistente varchar(10) not null,
likeDisl char (1),
comentarios varchar (500),
compartidos int,
foreign key (qrAsistente) references QRASISTENCIA (idQr)
);
go
