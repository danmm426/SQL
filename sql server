--CREATE-----------------------------
if OBJECT_ID('nuevoUsuario') is not null
drop procedure nuevoUsuario
go
create procedure nuevoUsuario
@idUsuario int,
@nombres nvarchar(30),
@apellidos nvarchar(30),
@usuario varchar(30),
@pass varchar(10),
@edad int,
@correo varchar(30),
@rol varchar(20) = 'regular',
@politicaData char(1) = '1',
@noSeguidos int = '',
@razonSocial nvarchar (30),
@noSeguidores int = ''
	as
	insert into USUARIOS (idUsuario,nombres,apellidos,usuario,pass,edad,correo,rol,politicaData,noSeguidos,razonSocial,noSeguidores) 
	values (@idUsuario,@nombres,@apellidos,@usuario,@pass,@edad,@correo,@rol,@politicaData,@noSeguidos,@razonSocial,@noSeguidores);
go

exec nuevoUsuario 1000000,'pepito','perez','pepito2022','qwerty',20,'pepito@gmail.com','regular','1',0,'pepito C.I.A',0

select * from USUARIOS

--DELETE-----------------------------

if OBJECT_ID('eliminarCuenta') is not null
	drop procedure eliminarCuenta
go
create procedure eliminarCuenta
@noIdentif int
	as
	delete from USUARIOS
	where idUsuario = @noIdentif
go
exec eliminarCuenta 1000000


--READ-------------------------------

if OBJECT_ID('numSeguidores') is not null
	drop procedure numSeguidores
go
create procedure numSeguidores
@o int
	as
	select count (*) as numSeguidores from SEGUIMIENTO
	group by seguido having seguido = @o

go

exec numSeguidores 76324843

--CREATE, UPDATE-------------------------------


if OBJECT_ID('seguirUsuario') is not null
	drop procedure seguirUsuario
go
create procedure seguirUsuario
 @usuarioAseguir int,
 @seguidorActivo int
as
	insert into SEGUIMIENTO (seguido,seguidor) values (@usuarioAseguir,@seguidorActivo);
	update USUARIOS set noSeguidos = noSeguidos+1
	where idUsuario = @seguidorActivo;
	update USUARIOS set noSeguidores = noSeguidores+1
	where idUsuario = @usuarioAseguir;
go

select idUsuario, noSeguidos, noSeguidores from USUARIOS
exec seguirUsuario 34570500,42870562
select idUsuario, noSeguidos, noSeguidores from USUARIOS

